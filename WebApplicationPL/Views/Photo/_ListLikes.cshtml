@model IEnumerable<BLL.Entities.BLLUser>

@if (Model.Count() == 0)
{
    <li>None</li>
}

@foreach (var user in Model)
{
    <li>
        <a href=@Url.Action(user.Id.ToString(), "Profile") class="like-list-item">
            <img src=@Url.Action("Avatar", "Photo", new { userId = user.Id }) width="50" height="50" class="img-circle">
            @user.Login
        </a>
    </li>
}

